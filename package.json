{
  "name": "@huluvu424242/honey-news",
  "version": "0.0.6",
  "description": "News reader web component for custom urls.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/components/index.d.ts",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/honey-news/honey-news.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Huluvu424242/honey-news.git"
  },
  "keywords": [
    "honey",
    "news",
    "rss",
    "atom",
    "webcomponent",
    "custom-element"
  ],
  "author": "Huluvu424242 alias FunThomas424242 and in real life Thomas Schubert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Huluvu424242/honey-news/issues"
  },
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test:local": "PWDEBUG=console playwright test",
    "test:win": "concurrently \"npm start\" \"set PWDEBUG=console && playwright test\"",
    "test": "playwright test",
    "test:pact": "npx jest .*.pact.ts",
    "test:e2e": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "prepare-deliver": "npm run build",
    "deliver": "npm publish --access=public",
    "pruefeVersionen": "npm outdated",
    "check-credits": "npx license-checker --failOn 'AGPL' --production >credits.txt"
  },
  "engines": {
    "node": "16.13.1"
  },
  "dependencies": {
    "@huluvu424242/liona-feeds": "2.3.3",
    "feedme": "2.0.2",
    "object-hash": "2.2.0",
    "rxjs": "7.4.0"
  },
  "devDependencies": {
    "concurrently":"6.5.1",
    "@stencil/core": "2.12.0",
    "@stencil/store": "1.5.0",
    "@types/jest": "27.0.3",
    "@playwright/test": "1.17.1",
    "jest": "27.4.5",
    "jest-cli": "27.4.5",
    "babel-jest": "27.4.5",
    "axios": "0.24.0",
    "@babel/preset-env": "7.16.7",
    "@babel/preset-modules": "0.1.5",
    "@babel/preset-typescript": "7.16.7",
    "jest-playwright-preset": "1.7.0",
    "playwright": "1.17.1",
    "expect-playwright": "0.8.0",
    "@pact-foundation/pact": "9.17.1",
    "ts-node": "10.4.0",
    "typescript": "4.5.4",
    "workbox-build": "4.3.1",
    "license-checker": "25.0.1"
  }
}
